{{ define "content" }}
<div class="jumbotron full_page_centered_content">
    <h1>Alistair J Madden</h1>
</div>
<h1>{{.PageTitle}}</h1>
<ul>
    {{- range .Todos -}}
        {{- if .Done -}}
            <li class="done">{{.Title}}</li>
        {{- else -}}
            <li>{{.Title}}</li>
        {{- end -}}
    {{- end -}}
</ul>
{{ end }}